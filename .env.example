# Typesense configuration
DOCA_TYPESENSE_HOST=http://typesense:8108
DOCA_TYPESENSE_API_KEY=xyz
DOCA_COLLECTION_NAME=doca_documents

# Chunking and indexing parameters
DOCA_CHUNK_SIZE=512
DOCA_CHUNK_OVERLAP=128
DOCA_BATCH_SIZE=50

# File processing limits
DOCA_MAX_CONTENT_LENGTH=100000
DOCA_MAX_CHUNKS_PER_FILE=50
DOCA_EFFECTIVE_CHUNK_SIZE=512
DOCA_EFFECTIVE_CHUNK_OVERLAP=128

# Memory optimization
DOCA_USE_CUDA=0
DOCA_MEMORY_LIMIT=8.0
